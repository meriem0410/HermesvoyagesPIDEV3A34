{% block content %}
<div class="col-lg-6 grid-margin stretch-card">
   
        <div class="card-body">

            <form method="post">
                {{ form_start(form) }}
                <div class="form-group">
                    {{ form_widget(form) }}
                </div>
                <button type="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>
                {{ form_end(form) }}
            </form>
        </div>
  
</div>
{% endblock %}
